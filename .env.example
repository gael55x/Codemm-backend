# Backend HTTP port
PORT=4000

# JWT Secret for authentication (change in production!)
JWT_SECRET=your-secret-key-change-in-production

# Docker judge timeout cap (ms)
JUDGE_TIMEOUT_MS=15000

# LLM provider selection (auto chooses based on which key is set)
CODEX_PROVIDER=auto

# Required to store per-user LLM API keys encrypted at rest.
# Must be 32 bytes base64 (recommended) or 64 hex chars.
# Example (macOS/Linux): `node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"`
CODEMM_USER_KEY_ENCRYPTION_KEY=your-32-byte-base64-key

# OpenAI / OpenAI-compatible (preferred env var name is CODEX_API_KEY)
CODEX_API_KEY=your-openai-api-key
# OPENAI_API_KEY=your-openai-api-key
# CODEX_BASE_URL=https://your-openai-compatible-endpoint
# CODEX_MODEL=gpt-4.1

# Anthropic
# ANTHROPIC_API_KEY=your-anthropic-api-key
# ANTHROPIC_MODEL=claude-3-5-sonnet-latest
# ANTHROPIC_BASE_URL=https://api.anthropic.com

# Gemini
# GEMINI_API_KEY=your-gemini-api-key
# GOOGLE_API_KEY=your-gemini-api-key
# GEMINI_MODEL=gemini-1.5-pro
# GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta
